{
  "version": 1,
  "paths": {
    "/getToken": {
      "name": "/getToken",
      "lambdaFunction": "amplifyGetUser",
      "permissions": {
        "setting": "open"
      }
    },
    "/sendData": {
      "name": "/sendData",
      "lambdaFunction": "sendData",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}